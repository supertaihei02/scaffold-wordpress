{
  "name": "scaffold-wordpress",
  "version": "0.4.2",
  "main": "index.js",
  "author": "kaave <abe@framelunch.jp>",
  "license": "MIT",
  "scripts": {
    "postinstall": "flow-typed install && npm run build:dll",
    "start": "NODE_ENV=development gulp",
    "build": "NODE_ENV=production gulp",
    "build:customizer": "NODE_ENV=production gulp customizer",
    "build:dll": "webpack --progress --config tools/webpack/dll.js",
    "check": "run-s typecheck lint",
    "lint": "run-p lint:*",
    "lint:script": "eslint --ext .js,.jsx frontend/",
    "lint:style": "stylelint 'frontend/**/*.css'",
    "typecheck": "flow",
    "safe-guard": "lint-staged",
    "fix": "run-p fix:*",
    "fix:style": "prettier --write 'frontend/styles/**/*.css'",
    "fix:script": "eslint --ext .ts --fix 'frontend/scripts/**/*.{js,jsx}'",
    "precommit": "run-s typecheck safe-guard"
  },
  "browserslist": [
    "ie 11",
    "ios >= 10",
    "> 1% in JP",
    "not ie <= 10",
    "not chrome 49"
  ],
  "lint-staged": {
    "subTaskConcurrency": 1,
    "linters": {
      "*.{js,jsx}": [
        "eslint --ext .js,.jsx --fix",
        "git add"
      ],
      "*.css": [
        "prettier --write",
        "stylelint",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "browser-sync": "^2.18.12",
    "cache-loader": "^1.1.0",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.4.0",
    "eslint-config-framelunch": "framelunch/eslint-config-framelunch#v0.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype-errors": "^3.3.6",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.52.0",
    "flow-typed": "^2.1.2",
    "globby": "^6.1.0",
    "gulp": "^3.9.1",
    "gulp-imagemin": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "husky": "^0.14.3",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.1",
    "lint-staged": "^6.1.1",
    "npm-run-all": "^4.0.2",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^10.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^2.0.2",
    "postcss-simple-vars": "^4.1.0",
    "postcss-url": "^7.0.0",
    "prettier": "^1.10.2",
    "raw-loader": "^0.5.1",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.1.0",
    "stylelint-config-framelunch": "framelunch/stylelint-config-framelunch#v0.2.0",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "date-fns": "^1.29.0",
    "jquery": "^3.2.1",
    "libraries-frontend-framelunch": "framelunch/libraries-frontend-framelunch#0.3.2"
  }
}
